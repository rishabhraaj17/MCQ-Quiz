<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rish.project"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="25" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.example.rish.project.AppController"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.rish.project.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.rish.project.LoginTab"
            android:label="@string/title_activity_login_tab"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.rish.project.RegisterTab"
            android:label="@string/title_activity_register_tab"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.example.rish.project.Home" />
        <activity android:name="com.example.rish.project.QuizPage" />
        <activity android:name="com.example.rish.project.ShowAllQuestions" />
        <activity android:name="com.example.rish.project.ContactUs" />
        <activity android:name="com.example.rish.project.Result" />
    </application>

</manifest>